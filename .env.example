# Example environment variables
PORT=3000
# Base chain RPC (Alchemy/Infura/Base mainnet)
BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/your-key
# Private key for signing transactions (do not commit real keys)
PRIVATE_KEY=0xabc123...
# Where prototype suggests sending ETH (mock or your address)
PAYMENT_ADDRESS=0x000000000000000000000000000000000000dEaD
# Neynar API key (for Farcaster), and signer UUID or any other required credentials
NEYNAR_API_KEY=your-neynar-api-key
NEYNAR_SIGNER_UUID=your-signer-uuid
