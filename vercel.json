{
  "routes": [
    {
      "src": "^/$",
      "dest": "/api/index.js"
    },
    {
      "src": "^/api/pay$",
      "dest": "/api/pay.js"
    },
    {
      "src": "^/.well-known/farcaster.json$",
      "status": 307,
      "headers": {
        "Location": "https://api.farcaster.xyz/miniapps/hosted-manifest/019a97fc-afc7-87c3-0d4b-5b89cd78d396"
      }
    }
  ]
}
